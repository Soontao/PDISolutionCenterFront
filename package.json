{
  "app": {
    "namespace": "ui5.psc.front",
    "resource": "https://openui5.hana.ondemand.com/resources/"
  },
  "name": "PDISolutionCenterFront",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run lint:fix",
    "start": "node_modules/.bin/gulp dev",
    "start:preload": "node_modules/.bin/gulp dev:preload",
    "build": "node_modules/.bin/gulp build",
    "build:sourcemap": "node_modules/.bin/gulp build:sourcemap",
    "lint:fix": "node_modules/.bin/eslint --fix \"src/**/*.js\"",
    "prepare": "npm run build",
    "release": "standard-version --commit-all --no-verify && git push --follow-tags origin master",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "precommit": "npm run build"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "standard-version": "^6.0.1",
    "husky": "^1.1.2",
    "@types/openui5": "^1.40.1",
    "@ui5-next/types": "latest",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-ui5-next": "^1.4.5",
    "browser-sync": "2.26.5",
    "del": "^2.2.2",
    "eslint": "^5.7.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.0.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-ui5-eager-preload": "^1.11.6",
    "http-proxy-middleware": "^0.19.1",
    "loader-utils": "^1.1.0",
    "merge-stream": "^1.0.1",
    "run-sequence": "2.2.1"
  }
}
